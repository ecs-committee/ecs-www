{
	"name": "ecs-www",
	"version": "0.1.19",
	"private": true,
	"release-it": {
		"github": {
			"release": false
		},
		"git": {
			"tagName": "v${version}",
			"commitMessage": "Release v${version}",
			"requireBranch": [
				"main",
				"master"
			]
		},
		"npm": {
			"publish": false
		}
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production npx next build && npx tsc --project tsconfig.server.json",
		"dev": "next dev",
		"postinstall": "prisma generate",
		"lint": "next lint",
		"start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
		"dev:wss": "cross-env NODE_ENV=development PORT=3001 npx tsx watch src/server/wssDevServer.ts",
		"staging:local": "docker-compose up --build -d",
		"docker:start": "if [ \"$DATABASE_PATH\" != \"\" ]; then mkdir -p $DATABASE_PATH ; fi && prisma db push && node dist/server/prodServer.js"
	},
	"dependencies": {
		"@blueprintjs/core": "^4.11.6",
		"@blueprintjs/icons": "^4.10.0",
		"@blueprintjs/popover2": "^1.8.1",
		"@blueprintjs/select": "^4.8.6",
		"@headlessui/react": "^1.7.3",
		"@heroicons/react": "^2.0.12",
		"@prisma/client": "^4.7.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tanstack/react-query": "^4.10.0",
		"@trpc/client": "10.0.0-rc.8",
		"@trpc/next": "10.0.0-rc.8",
		"@trpc/react-query": "10.0.0-rc.8",
		"@trpc/server": "10.0.0-rc.8",
		"@types/jsonwebtoken": "^8.5.9",
		"axios": "^1.1.3",
		"bcrypt": "^5.1.0",
		"cross-env": "^7.0.3",
		"ioredis": "^5.2.4",
		"jest": "^29.2.2",
		"jsondiffpatch": "^0.4.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"next": "13.0.0",
		"node-fetch": "^3.2.10",
		"normalize.css": "^8.0.1",
		"react": "18.2.0",
		"react-data-table-component": "^7.5.3",
		"react-dom": "18.2.0",
		"styled-components": "^5.3.6",
		"superjson": "1.9.1",
		"tes": "github:trippelm/tes#typescript",
		"uuid": "^9.0.0",
		"ws": "^8.10.0",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/ioredis": "^5.0.0",
		"@types/lodash": "^4.14.187",
		"@types/node": "18.0.0",
		"@types/react": "18.0.14",
		"@types/react-dom": "18.0.5",
		"@types/uuid": "^8.3.4",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"autoprefixer": "^10.4.7",
		"eslint": "8.22.0",
		"eslint-config-next": "13.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.7.1",
		"prettier-plugin-prisma": "^4.4.0",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"prisma": "^4.7.0",
		"release-it": "^15.5.0",
		"tailwindcss": "^3.2.0",
		"typescript": "^4.8.4"
	},
	"ct3aMetadata": {
		"initVersion": "6.6.0"
	}
}
